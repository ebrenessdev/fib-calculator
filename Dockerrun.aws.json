{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "ebreness/training_fib_app:client",
            "hostname": "client",
            "essential": false,
            "memory": 128
        },
        {
            "name": "server",
            "image": "ebreness/training_fib_app:server",
            "hostname": "express-api",
            "essential": true,
            "memory": 128
        },
        {
            "name": "worker",
            "image": "ebreness/training_fib_app:worker",
            "essential": true,
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "ebreness/training_fib_app:nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["client", "server"],
            "memory": 128
        }
    ]
}
